# GhostLAN SimWorld - Environment Configuration
# Rename this file to .env and fill in your actual values

# Duality AI Configuration
# Get your API key from the Duality AI sponsor
DUALITY_AI_API_KEY=your-duality-ai-api-key-here
DUALITY_AI_API_URL=https://api.duality.ai

# JWT Configuration (for mobile app)
JWT_SECRET_KEY=ghostlan-secret-key-change-in-production

# Database Configuration
DATABASE_URL=sqlite:///ghostlan.db

# Cloud Configuration (optional)
# AWS_ACCESS_KEY_ID=your-aws-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret
# AZURE_CONNECTION_STRING=your-azure-connection-string
# GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# Development Configuration
DEBUG=true
LOG_LEVEL=INFO 